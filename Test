/**
 *
 * @author RandyNguyen
 */
public class main {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {

        for (int n = 0; n < 5; n++) {
            for (int col = 0; col < n; col++) {
                System.out.print("#");
            }

            System.out.println();
        }
    }
}
